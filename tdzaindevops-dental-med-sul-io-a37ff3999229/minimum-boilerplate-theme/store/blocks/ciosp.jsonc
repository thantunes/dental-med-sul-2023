        "props": {
          "blockClass": "bloco-titulo-perguntas-frequentes-faq-ciosp"
        }
      },
      "rich-text#perguntasfrequentesfaq": {
        "props": {
          "text": "Perguntas Frequentes CIOSP",
          "blockClass": "titulo-perguntas-frequentes-faq-ciosp"
        }
