    "list-context.product-list#shelf-ofertas-da-semana-ciosp": {
      "blocks": ["product-summary.shelf"],
      "children": ["slider-layout#shelf-ofertas-da-semana-ciosp"]
    },
    "slider-layout#shelf-ofertas-da-semana-ciosp": {
      "props": {
        "label": "Products",
        "itemsPerPage": {
          "desktop": 6,
          "tablet": 4,
          "phone": 2
        },
        "showPaginationDots": "never",
        "blockClass": ["productShelf"],
        "infinite": true,
        "arrowSize": {
          "desktop": 15,
          "mobile": 15
        }
      }
    },
    // Slider da vitrine
    "slider-layout#shelf": {
      "props": {
        "label": "Products",
        "itemsPerPage": {
          "desktop": 6,
          "tablet": 4,
          "phone": 2
        },
        "showPaginationDots": "never",
        "blockClass": ["productShelf"],
        "infinite": true,
        "arrowSize": {
          "desktop": 15,
          "mobile": 15
        }
      }
