  "flex-layout.row#newsletter": {
    "props": {
      "paddingTop": 8,
      "blockClass": [
        "footerRow"
      ]
    },
    "children": [
      "newsletter-form"
    ]
  },
  "flex-layout.row#footer": {
    "props": {
      "colSizing": "auto",
      "paddingTop": 4,
      "paddingBottom": 4,
      "blockClass": [
        "footerRow"
      ]
