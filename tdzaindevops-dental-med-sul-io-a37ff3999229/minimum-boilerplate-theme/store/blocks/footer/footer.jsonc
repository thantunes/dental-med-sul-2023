    "children": [
      "rich-text#attendance",
      "rich-text#number",
      "rich-text#schedule"
    ]
  },
  "flex-layout.col#footerSocial": {
    "props": {
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "colSizing": "auto",
      "horizontalAlign": "center",
